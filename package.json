{
  "name": "wp-mini-template",
  "version": "1.0.0",
  "author": "Misza777",
  "description": "Misza777 webpack template with HMR, sass, jquery",
  "repository": {
    "type": "git",
    "url": "https://github.com/https://github.com/misza777/Misza777_Webpack_Template"
  },
  "main": "index.js",
  "scripts": {
